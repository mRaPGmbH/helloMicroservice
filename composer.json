{
    "name": "hellocash/hellomicroservice",
    "description": "Shared tools for laravel/lighthouse/graphql based webservices, inlcuding RSA JWT auth and multitenancy through JWT claim and laravel scopes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Huber",
            "email": "michael.huber@hellocash.at"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HelloCash\\HelloMicroservice\\": "src/"
        }
    },
    "require": {
        "umpirsky/country-list": "^2.0",
        "nuwave/lighthouse": "^4.14",
        "sentry/sentry-laravel": "1.8.0",
        "laravel/scout": "^8.2",
        "tymon/jwt-auth": "^1.0",
        "yab/laravel-scout-mysql-driver": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelloCash\\HelloMicroservice\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
